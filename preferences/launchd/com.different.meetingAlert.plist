<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>meetingAlert</string>
    <key>UserName</key>
    <string>alister</string>
    <key>Program</key>
    <string>source /Users/alister/.bashrc; _meetingAlert</string>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Minute</key>
        <integer>1</integer>
    </dict>
    <key>Debug</key>
    <true/>
    <key>AbandonProcessGroup</key>
    <true/>
</dict>
</plist>
